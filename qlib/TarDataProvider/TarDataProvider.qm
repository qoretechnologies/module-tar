#! TarDataProvider factory class
public class TarDataProviderFactory inherits AbstractDataProviderFactory {
    public {
        #! Factory info
        const FactoryInfo = <DataProviderFactoryInfo>{
            "name": "tar",
            "desc": "TAR archive data provider factory",
            "children_can_support_apis": True,
        };
    }

    #! Returns the factory name
    string getName() {
        return FactoryInfo.name;
    }

    #! Returns static factory information
    hash<DataProviderFactoryInfo> getInfoImpl() {
        return FactoryInfo;
    }

    #! Returns the data provider info
    private hash<DataProviderInfo> getProviderInfoImpl() {
        return TarDataProvider::ProviderInfo;
    }

    #! Returns a data provider with the given options
    private AbstractDataProvider getProviderImpl(hash<auto> opts) {
        return new TarDataProvider(opts);
    }

    #! Returns the class for the data provider
    private Class getClassImpl() {
        return Class::forName("TarDataProvider::TarDataProvider");
    }
}

#! Main TAR data provider class
public class TarDataProvider inherits AbstractDataProvider {
    public {
        #! Provider info
        const ProviderInfo = <DataProviderInfo>{
            "name": "tar",
            "desc": "TAR archive data provider",
            "type": "TarDataProvider",
            "supports_children": True,
            "children_can_support_apis": True,
        };
    }

    private {
        #! Options passed to constructor
        hash<auto> opts;
    }

    #! Constructor
    constructor(hash<auto> opts = {}) {
        self.opts = opts;
    }

    #! Returns the data provider name
    string getName() {
        return ProviderInfo.name;
    }

    #! Returns the data provider description
    *string getDesc() {
        return ProviderInfo.desc;
    }

    #! Returns child providers
    private *list<string> getChildProviderNamesImpl() {
        return ("archive", "file", "data", "directory", "stream", "entry");
    }

    #! Returns the given child provider or NOTHING if it doesn't exist
    private *AbstractDataProvider getChildProviderImpl(string name) {
        switch (name) {
            case "archive":
                return new TarArchiveDataProvider(opts);
            case "file":
                return new TarFileDataProvider(opts);
            case "data":
                return new TarDataDataProvider(opts);
            case "directory":
                return new TarDirectoryDataProvider(opts);
            case "stream":
                return new TarStreamDataProvider(opts);
            case "entry":
                return new TarEntryDataProvider(opts);
        }
        return NOTHING;
    }

    #! Returns data provider static info
    private hash<DataProviderInfo> getStaticInfoImpl() {
        return ProviderInfo;
    }
}

