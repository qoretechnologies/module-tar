#! Compression method names for user-friendly input
const CompressionMethods = {
    "none": TAR_CM_NONE,
    "gzip": TAR_CM_GZIP,
    "bzip2": TAR_CM_BZIP2,
    "xz": TAR_CM_XZ,
    "zstd": TAR_CM_ZSTD,
    "lz4": TAR_CM_LZ4,
};

#! Format names for user-friendly input
const TarFormats = {
    "ustar": TAR_FORMAT_USTAR,
    "pax": TAR_FORMAT_PAX,
    "gnu": TAR_FORMAT_GNU,
    "v7": TAR_FORMAT_V7,
};

#! Helper to get compression method from string
public int sub getCompressionMethod(auto val) {
    if (!exists val) {
        return TAR_CM_NONE;
    }
    if (val.typeCode() == NT_INT) {
        return int(val);
    }
    string method = string(val).lwr();
    return CompressionMethods{method} ?? TAR_CM_NONE;
}

#! Helper to get format from string
public int sub getTarFormat(auto val) {
    if (!exists val) {
        return TAR_FORMAT_PAX;
    }
    if (val.typeCode() == NT_INT) {
        return int(val);
    }
    string fmt = string(val).lwr();
    return TarFormats{fmt} ?? TAR_FORMAT_PAX;
}

#! List archive contents data provider
public class TarListArchiveDataProvider inherits DataProvider::AbstractDataProvider {
    public {
        const ProviderInfo = <DataProviderInfo>{
            "name": "list",
            "desc": "List contents of a TAR archive",
            "type": "TarListArchiveDataProvider",
            "supports_request": True,
        };
    }

    private {
        hash<auto> opts;
    }

    constructor(hash<auto> opts = {}) {
        self.opts = opts;
    }

    string getName() {
        return ProviderInfo.name;
    }

    private hash<DataProviderInfo> getStaticInfoImpl() {
        return ProviderInfo;
    }

    private *AbstractDataProviderType getRequestTypeImpl() {
        return new TarListRequestDataType();
    }

    private *AbstractDataProviderType getResponseTypeImpl() {
        return new TarListResponseDataType();
    }

    private auto doRequestImpl(auto req, *hash<auto> request_options) {
        string archive_path;
        *binary archive_data;

        if (req.archive_path) {
            archive_path = req.archive_path;
        }
        if (req.archive_data) {
            archive_data = req.archive_data;
        }

        TarFile tar;
        if (archive_data) {
            tar = new TarFile(archive_data);
        } else if (archive_path) {
            tar = new TarFile(archive_path, "r");
        } else {
            throw "TAR-ERROR", "Either archive_path or archive_data must be provided";
        }

        list<hash<TarEntryInfo>> entries = tar.entries();
        tar.close();

        return {
            "entries": entries,
            "count": entries.size(),
        };
    }
}

